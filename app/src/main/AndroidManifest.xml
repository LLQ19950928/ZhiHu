<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.llq.zhihu">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <application
        android:allowBackup="true"
        android:name=".ZhiHuApplication"
        android:icon="@mipmap/zhihu"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="view.activity.StartActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="view.activity.MainActivity"/>
        <activity android:name="view.activity.NewsContentActivity"/>
        <activity android:name="view.activity.ThemeContentActivity"/>
        <activity android:name="view.activity.EditorActivity"/>
        <activity android:name="view.activity.CommentActivity"/>
    </application>

</manifest>